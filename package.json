{
  "name": "react-render-markup",
  "version": "3.6.1",
  "description": "Safely parse HTML, SVG and MathML into React elements.",
  "type": "module",
  "files": [
    "lib"
  ],
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.esm.js",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "benchmark": "node benchmark",
    "build": "rollup -c",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "npm run build && husky install",
    "preversion": "npm run lint && npm run test",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobbuck/react-render-markup.git"
  },
  "keywords": [
    "html",
    "markup",
    "react",
    "render"
  ],
  "author": "Jacob Buck <buck.jacob@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacobbuck/react-render-markup/issues"
  },
  "homepage": "https://github.com/jacobbuck/react-render-markup#readme",
  "dependencies": {
    "css-to-style": "^1.4.0",
    "dom-parse": "^3.0.0",
    "prop-types": "^15.8.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.6",
    "@babel/preset-env": "^7.22.6",
    "@rollup/plugin-babel": "^6.0.3",
    "babel-jest": "^29.6.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-test-renderer": "^18.0.0",
    "rollup": "^3.26.1",
    "tinybench": "^2.5.0"
  },
  "peerDependencies": {
    "react": "^16.6.0 || 17 || 18"
  }
}
