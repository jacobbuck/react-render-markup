{
  "name": "react-render-markup",
  "version": "1.2.2",
  "description": "Safely parse HTML, SVG and MathML into React elements.",
  "browser": "lib/browser.js",
  "main": "lib/browser.js",
  "module": "lib/server.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rollup -c",
