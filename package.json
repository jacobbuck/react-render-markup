  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobbuck/react-render-markup.git"
  },
  "keywords": [
    "html",
    "markup",
    "react",
    "render",
    "xhtml"
  ],
  "author": "Jacob Buck <buck.jacob@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacobbuck/react-render-markup/issues"
  },
  "homepage": "https://github.com/jacobbuck/react-render-markup#readme",
  "dependencies": {
    "css-to-style": "^1.3.0",
    "html-void-elements": "^1.0.5",
    "jsdom": "^16.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-replace": "^2.3.2",
    "babel-jest": "^25.4.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.4.0"
  },
  "peerDependencies": {
    "react": ">=15.3.0"
